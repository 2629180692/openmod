<Project Sdk="Microsoft.NET.Sdk">
 
  <PropertyGroup>
    <PackageId>OpenMod.Rocket.Unturned</PackageId>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <PackageDescription>Rocket.Unturned integration for OpenMod</PackageDescription>
  	<PackageTags>openmod openmod-plugin unturned rocketmod</PackageTags>
    <AssemblyVersion>4.9.3.3</AssemblyVersion>
    <TargetFramework>net461</TargetFramework>
    <Copyright>Sven Mawby</Copyright>
    <Authors>Sven Mawby;OpenMod</Authors>
    <Company>OpenMod</Company>
  </PropertyGroup>

  <Import Project="$(MSBuildThisFileDirectory)..\props\SharedProjectProps.props" />
	
  <ItemGroup>
    <PackageReference Include="OpenMod.Unturned" Version="2.0.7" />
    <PackageReference Include="Microsoft.NETFramework.ReferenceAssemblies.net461" Version="1.0.0">
      <PrivateAssets>all</PrivateAssets>
      <IncludeAssets>runtime; build; native; contentfiles; analyzers; buildtransitive</IncludeAssets>
    </PackageReference>
    <PackageReference Include="OpenMod.Unturned.Redist" Version="3.20.9" />
  </ItemGroup>
  <ItemGroup>
    <ProjectReference Include="..\Rocket.API\Rocket.API.csproj" />
    <ProjectReference Include="..\Rocket.Core\Rocket.Core.csproj" />
  </ItemGroup>
</Project>