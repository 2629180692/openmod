<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <PackageId>$(AssemblyName)</PackageId>
    <PackageRequireLicenseAcceptance>true</PackageRequireLicenseAcceptance>
    <PackageTags>RocketMod Rocket UnityEngine UnrealEngine Plugin Framework</PackageTags>
    <PackageLicenseExpression>EUPL-1.2</PackageLicenseExpression>
    <PackageProjectUrl>https://get-openmod.com/</PackageProjectUrl>
    <PackageOwners>Enes Sadık Özbek&lt;esozbek.me&gt;</PackageOwners>
    <Authors>Enes Sadık Özbek &lt;esozbek.me&gt;</Authors>
	<Version>0.0.0</Version>
    <Company>OpenMod</Company>
    <Copyright>Enes Sadık Özbek &lt;esozbek.me&gt;</Copyright>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <LangVersion>8</LangVersion>
  </PropertyGroup>
  
  <ItemGroup Condition="'$(TargetFramework)'== 'net461'">
    <PackageReference Include="Microsoft.NETFramework.ReferenceAssemblies" Version="1.0.0" PrivateAssets="All" />
  </ItemGroup>
</Project>