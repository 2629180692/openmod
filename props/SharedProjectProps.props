<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <PackageId>$(AssemblyName)</PackageId>
    <PackageRequireLicenseAcceptance>true</PackageRequireLicenseAcceptance>
    <PackageTags>RocketMod Rocket UnityEngine UnrealEngine Plugin Framework</PackageTags>
    <PackageLicenseUrl>https://github.com/openmod/OpenMod/blob/master/LICENSE</PackageLicenseUrl>
    <PackageProjectUrl>https://get-openmod.com/</PackageProjectUrl>
    <PackageOwners>Enes Sadık Özbek&lt;esozbek.me&gt;</PackageOwners>
    <Authors>Enes Sadık Özbek &lt;esozbek.me&gt;</Authors>
	<Version>0.0.0</Version>
    <Company>OpenMod</Company>
    <Copyright>Enes Sadık Özbek &lt;esozbek.me&gt;</Copyright>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
  </PropertyGroup>
  
  <ItemGroup Condition="'$(TargetFramework)'== 'net461'">
    <PackageReference Include="Microsoft.NETFramework.ReferenceAssemblies.net461" Version="1.0.0">
      <PrivateAssets>all</PrivateAssets>
      <IncludeAssets>runtime; build; native; contentfiles; analyzers; buildtransitive</IncludeAssets>
    </PackageReference>
  </ItemGroup>

  <PropertyGroup Condition="'$(TargetFramework)'== 'net461'">
    <FrameworkPathOverride>$(NuGetPackageRoot)microsoft.targetingpack.netframework.v4.6.1/1.0.1/lib/net461/</FrameworkPathOverride>
  </PropertyGroup>
</Project>